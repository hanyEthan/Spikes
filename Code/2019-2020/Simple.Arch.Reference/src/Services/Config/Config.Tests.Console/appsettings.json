{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId", "WithExceptionDetails" ],
    "WriteTo": [
      { "Name": "Debug" },
      { "Name": "Console" },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://10.0.2.14:9200/",
          "indexFormat": "mcs-invoicing-{0:yyyy.MM}",
          "autoRegisterTemplate": true,
          "autoRegisterTemplateVersion": "ESv6"

        }
      }
    ]
  },

  "Async.Service": {
    "Transport": {
      "Uri": "rabbitmq://localhost/",
      "CredentialsUsername": "guest",
      "CredentialsPassword": "guest"
    }
  },

  "Services": {
    "Config": {
      "Client.Async.Messages": {
        "ServiceEndpoint": "Mcs.Invoicing.Config.Create.Queue"
      },
      "Client.gRPC": {
        "ServiceEndpoint": "https://localhost:44340"
      },
      "Client.Rest": {
        "ServiceEndpoint": "https://localhost:44340"
      }
    }
  }
}
